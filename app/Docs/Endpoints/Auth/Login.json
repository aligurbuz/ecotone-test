{
    "name": "Auth",
    "item": [
        {
            "name": "Login",
            "protocolProfileBehavior": {
                "disabledSystemHeaders": {
                    "content-type": true
                }
            },
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Authorization",
                        "value": "Bearer {{bearerToken}}",
                        "type": "text",
                        "disabled": true
                    },
                    {
                        "key": "Apikey",
                        "value": "{{apiKey}}",
                        "type": "text"
                    },
                    {
                        "key": "Content-Type",
                        "value": "{{contentType}}",
                        "type": "text"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "[\n    {\n        \"email\": \"test@gmail.com\",\n        \"password\": \"123456\"\n    }\n]",
                    "options": {
                        "raw": {
                            "language": "json"
                        }
                    }
                },
                "url": {
                    "raw": "{{baseUrl}}/login",
                    "host": [
                        "{{baseUrl}}"
                    ],
                    "path": [
                        "login"
                    ]
                }
            },
            "response": []
        }
    ]
}
